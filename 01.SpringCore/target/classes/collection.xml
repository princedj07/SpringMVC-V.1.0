<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	 http://www.springframework.org/schema/beans/spring-beans.3.1.xsd
	 http://www.springframework.org/schema/context 
	 http://www.springframework.org/schema/context/spring-context-3.1.xsd ">

	<bean name="emp1" class = "org.spring.core.app.collections.Employee">
		<property name="nane" value="Ram" />

		<property name="phones">
			<list>
				<value>020304050</value>
				<value>7083751750</value>
				<value>9309725333</value>
			</list>
		</property>

		<property name="addresses">
			<set>
				<value>Delhi</value>
				<value>Mumbai</value>
				<value>Pune</value>
			</set>
		</property>

		<property name="cources">
			<map>
				<entry key = "Java" value = "2Month" />
				<entry key = "Python" value = "3Month" />
				<entry key = "C++" value = "1Month" />
			</map>
		</property>
	</bean>

</beans>
